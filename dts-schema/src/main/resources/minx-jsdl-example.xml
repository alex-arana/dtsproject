<?xml version="1.0" encoding="UTF-8"?>
<jsdl:JobDefinition xmlns:mjsdl="http://schemas.dataminx.org/dts/2009/07/jsdl"
	xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl"
	xmlns:dmi="http://schemas.ogf.org/dmi/2008/05/dmi"
	xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://schemas.ggf.org/jsdl/2005/11/jsdl jsdl-redef.xsd
	                    http://schemas.dataminx.org/dts/2009/07/jsdl minx-jsdl.xsd
	                    http://schemas.ogf.org/dmi/2008/05/dmi dmi-redef.xsd
	                    http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
	<jsdl:JobDescription xsi:type="mjsdl:MinxJobDescriptionType"  xmlns:mjsdl="http://schemas.dataminx.org/dts/2009/07/jsdl" xmlns:jsdl="http://schemas.ggf.org/jsdl/2005/11/jsdl">
		<jsdl:JobIdentification>
			<jsdl:JobName>p:JobName</jsdl:JobName>
			<jsdl:Description>A Valid MINX JSDL Document</jsdl:Description>
			<jsdl:JobAnnotation>p:JobAnnotation</jsdl:JobAnnotation>
			<jsdl:JobProject>p:JobProject</jsdl:JobProject>
		</jsdl:JobIdentification>
		<mjsdl:DataTransfer>
			<mjsdl:Source>
			  <jsdl:URI>gsiftp://ng2.vpac.org/etc/termcap</jsdl:URI>
        <mjsdl:Credential>
            <mjsdl:MyProxyToken>
                <mjsdl:MyProxyUsername>${myproxy.username}</mjsdl:MyProxyUsername>
                <mjsdl:MyProxyPassword>${myproxy.password}</mjsdl:MyProxyPassword>
                <mjsdl:MyProxyServer>myproxy2.arcs.org.au</mjsdl:MyProxyServer>
                <mjsdl:MyProxyPort>7512</mjsdl:MyProxyPort>
            </mjsdl:MyProxyToken>
        </mjsdl:Credential>
			</mjsdl:Source>
			<mjsdl:Target>
			  <!--<jsdl:URI>file:///tmp/lcg-info-generic-vpac-brecca.conf</jsdl:URI>-->
        <jsdl:URI>ftp://dm11.intersect.org.au/upload/termcap-from-vpac</jsdl:URI>
        <mjsdl:Credential>
            <wsse:UsernameToken>
              <wsse:Username>${ftp.username}</wsse:Username>
              <wsse:PasswordString>${ftp.password}</wsse:PasswordString>
            </wsse:UsernameToken>
        </mjsdl:Credential>
			</mjsdl:Target>
      <mjsdl:TransferRequirements>
        <dmi:MaxAttempts>0</dmi:MaxAttempts>
        <jsdl:CreationFlag>overwrite</jsdl:CreationFlag>
      </mjsdl:TransferRequirements>
		</mjsdl:DataTransfer>
	</jsdl:JobDescription>
</jsdl:JobDefinition>
