<?xml version="1.0" encoding="UTF-8"?>
<submitJobRequest xmlns="http://schemas.dataminx.org/dts/2009/05/dts"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="minx-dts.xsd">

  <JobDefinition>
    <JobDescription>
      <JobIdentification>
        <JobName>DTSJob0059</JobName>
        <Description>Copies the DataMINX Logo from a HTTP source to a local folder</Description>
        <JobAnnotation>DTS Job Annotation</JobAnnotation>
        <JobProject>DataMINX File Upload</JobProject>
      </JobIdentification>
      <DataTransfer>
        <Source>
          <!--<URI>file:///D:/Temp/metadata.txt</URI>-->
          <URI>http://wiki.arcs.org.au/pub/DataMINX/DataMINX/minnie_on_the_run.jpg</URI>
          <Credential/>
          <URIProperties/>
        </Source>
        <Target>
          <!--<URI>file:///L:/Shared/Alex/metadata.txt</URI>-->
          <URI>file:///C:/DataMINX-logo.jpg</URI>
          <Credential/>
          <URIProperties/>
        </Target>
        <TransferRequirements>
          <MaxAttempts>0</MaxAttempts>
          <CreationFlag>Overwrite</CreationFlag>
        </TransferRequirements>
      </DataTransfer>
    </JobDescription>
  </JobDefinition>
</submitJobRequest>
