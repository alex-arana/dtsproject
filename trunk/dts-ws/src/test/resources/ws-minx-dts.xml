<?xml version="1.0" encoding="UTF-8"?>
<JobDefinition xmlns="http://schemas.dataminx.org/dts/2009/05/dts" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.dataminx.org/dts/2009/05/dts minx-dts.xsd">
  <JobDescription>
    <JobIdentification>
      <JobName>DTS_JOB_ID</JobName>
      <Description>Copies the DataMINX Logo from a HTTP source to a local folder</Description>
      <JobAnnotation>DTS Job Annotation</JobAnnotation>
      <JobProject>DataMINX File Upload</JobProject>
    </JobIdentification>
    <DataTransfer>
      <Source>
        <!--<URI>http://wiki.arcs.org.au/pub/DataMINX/WebPreferences/favicon.ico</URI>-->
        <URI>ftp://mirror.pacific.net.au/linux/ubuntu/dists/jaunty/Contents-i386.gz</URI>
        <Credential/>
        <URIProperties/>
      </Source>
      <Target>
        <URI>file:///tmp/Contents-i386.gz</URI>
        <Credential/>
        <URIProperties/>
      </Target>
      <TransferRequirements>
        <MaxAttempts>0</MaxAttempts>
        <CreationFlag>Overwrite</CreationFlag>
      </TransferRequirements>
    </DataTransfer>
  </JobDescription>
</JobDefinition>
