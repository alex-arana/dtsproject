si.jms.brokerURL = tcp://localhost:61616

si.jms.brokerJobSubmissionQueue=broker.job.submission.queue
si.jms.brokerDefaultOutputQueue = broker.default.output.queue

# Header name for scheduling purpose
broker.delay.headerName = broker.delay.headerName
broker.xml.queryStr = declare namespace dmi='http://schemas.ogf.org/dmi/2008/05/dmi';$this//dmi:StartNotBefore

# Header name in spring message for routing purpose
si.jms.routingHeader=springintegration_jms_routingHeader

# JMS listener container caching settings
si.jms.sessionCacheSize = 10
si.jms.cacheProducers = false