<?xml version="1.0" encoding="UTF-8"?>
<JobDefinition xmlns="http://schemas.dataminx.org/dts/2009/05/dts" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://schemas.dataminx.org/dts/2009/05/dts minx-dts.xsd ">
  <JobDescription>
    <JobIdentification>
      <JobName>DTS_JOB_ID</JobName>
      <Description>some job description</Description>
      <JobAnnotation>DTS Job Annotation</JobAnnotation>
      <JobProject>DataMINX File Upload</JobProject>
    </JobIdentification>
    <DataTransfer>
      <Source>
        <!--<URI>http://wiki.arcs.org.au/pub/DataMINX/WebPreferences/favicon.ico</URI>-->
        <URI>gsiftp://ng2.vpac.org/etc/termcap</URI>
        <Credential>
            <MyProxyToken>
                <MyProxyUsername>${myproxy.username}</MyProxyUsername>
                <MyProxyPassword>${myproxy.password}</MyProxyPassword>
                <MyProxyServer>myproxy2.arcs.org.au</MyProxyServer>
                <MyProxyPort>7512</MyProxyPort>
            </MyProxyToken>
        </Credential>
      </Source>
      <Target>
        <!--<URI>file:///tmp/lcg-info-generic-vpac-brecca.conf</URI>-->
        <URI>ftp://dm11.intersect.org.au/upload/termcap-from-vpac</URI>
        <Credential>
            <wsse:UsernameToken>
              <wsse:Username>${ftp.username}</wsse:Username>
              <wsse:PasswordString>${ftp.password}</wsse:PasswordString>
            </wsse:UsernameToken>
        </Credential>
      </Target>
      <TransferRequirements>
        <MaxAttempts>0</MaxAttempts>
        <CreationFlag>Overwrite</CreationFlag>
      </TransferRequirements>
    </DataTransfer>
  </JobDescription>
</JobDefinition>
